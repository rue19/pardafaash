{
  "manifest_version": 3,
  "name": "Deepfake Detector",
  "version": "1.0",
  "description": "Detects if images are deepfakes using Sightengine API.",
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "https://api.sightengine.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
